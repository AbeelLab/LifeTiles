<?xml version="1.0" encoding="UTF-8"?>

<?import java.net.*?>
<?import java.lang.*?>
<?import javafx.geometry.*?>
<?import javafx.scene.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.text.*?>
<?import javafx.scene.control.Label?>
<?import nl.tudelft.lifetiles.tree.view.SunburstView?>

<BorderPane xmlns:fx="http://javafx.com/fxml/1" minHeight="0.0"
	minWidth="0.0" prefHeight="160.0" prefWidth="100.0"
	fx:controller="nl.tudelft.lifetiles.tree.controller.TreeController"
	fx:id="wrapper">
	<top>
		<Label text="Tree Viewer">
			<padding>
				<Insets bottom="5.0" left="5.0" right="5.0" top="5.0" />
			</padding>
		</Label>
		
	</top>
	<center>
		<SunburstView fx:id="view"></SunburstView>
	</center>

	<stylesheets>
		<URL value="@../styles/TreeStyle.css" />
	</stylesheets>
</BorderPane>
